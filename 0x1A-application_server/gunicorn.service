[Unit]
Description=Gunicorn instance to serve AirBnB_clone project.
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/root/alx-system_engineering-devops/0x1A-application_server
Environment="PATH=/home/ubuntu/.local/bin/gunicorn"
ExecStart=/home/ubuntu/.local/bin/gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2> /tmp/airbnb-error.log

[Install]
WantedBy=multi-user.target